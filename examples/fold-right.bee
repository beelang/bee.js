fold-right:
  (function) ->
    (current) ->
      function-with-current: function(current)
      (collection) ->
        remaining: tail(collection)
        latest: head(collection)
        if(is-empty(collection))
          >- function-with-current(latest)
          >- sage sage(function)(function-with-current(latest))(remaining)


